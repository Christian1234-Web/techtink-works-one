{
	"extends": ["react-app", "react-app/jest"],
	"env": {
		"browser": true,
		"commonjs": true,
		"node": true,
		"es6": true
	},
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module"
	},
	"rules": {
		"no-console": "off",
		"strict": ["error", "global"],
		"curly": "warn",
		"jsx-a11y/click-events-have-key-events": "off",
		"jsx-a11y/anchor-is-valid": "off",
		"jsx-a11y/label-has-associated-control": "off",
		"jsx-a11y/interactive-supports-focus": "off"
	}
}
